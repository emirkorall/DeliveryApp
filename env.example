# Application Configuration
JPA_DDL_AUTO=update
JPA_SHOW_SQL=false
JPA_FORMAT_SQL=false

# Logging Configuration
LOGGING_LEVEL_COM_EMIRKORAL_DELIVERYAPP=DEBUG
LOGGING_LEVEL_ORG_SPRINGFRAMEWORK_SECURITY=DEBUG 